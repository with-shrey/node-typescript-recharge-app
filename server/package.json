{
  "name": "axom",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "ts-node api/index.ts",
    "startjs": "node out-tsc/server/api/index.js",
    "migration:make": "./node_modules/.bin/knex migrate:make",
    "migration:latest": "./node_modules/.bin/knex migrate:latest",
    "migration:rollback": "./node_modules/.bin/knex migrate:rollback",
    "seed": "./node_modules/.bin/knex seed:run",
    "dev": "nodemon --watch 'api/**/*.ts' --watch '../common/**/*.ts' --exec 'ts-node' api/index.ts"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "axios": "^0.19.0",
    "body-parser": "^1.20.1",
    "bunyan": "^1.8.8",
    "bunyan-prettystream": "^0.1.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.18.2",
    "express-joi-validation": "^4.0.3",
    "express-validator": "^6.3.0",
    "handlebars": "^4.5.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.4",
    "lodash": "^4.17.15",
    "mysql": "^2.17.1",
    "notp": "^2.0.3",
    "require-sql": "^1.0.0"
  },
  "devDependencies": {
    "@types/express-validator": "^3.0.0",
    "@types/axios": "^0.14.0",
    "@types/bunyan": "^1.8.6",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/handlebars": "^4.1.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.0",
    "@types/notp": "^2.0.0",
    "nodemon": "^2.0.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  }
}
